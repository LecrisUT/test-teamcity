<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="453bf35c-105f-4007-8a8f-e470d4fa1d0d" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Test build</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="testValue" />
      <option name="cleanBuild" value="true" />
    </options>
    <parameters>
      <param name="system.testName" value="" spec="text display='normal' validationMode='any'" />
      <param name="system.testValue" value="" spec="text display='normal' label='Values' validationMode='any'" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_13" name="" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[echo "Hello, this is test of [name=%system.testName%] with [value=%system.testValue%]"
tree
touch testValue
tail -n +0 ./*
echo "%system.testValue%" > testValue]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

