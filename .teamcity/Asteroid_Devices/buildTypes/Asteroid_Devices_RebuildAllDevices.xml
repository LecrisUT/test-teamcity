<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="21a7a3bf-7dda-4abf-98c4-8875a61b3d6b" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Rebuild All Devices</name>
  <description>Rebuild all devices from scratch and upload a new cache seed</description>
  <settings>
    <parameters />
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="Asteroid_Devices_MetaSmatwtach" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="Asteroid_Devices_Catfish_BuildImageFromScratch">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="run-build-if-dependency-failed-to-start" value="RUN_ADD_PROBLEM" />
          <option name="run-build-on-the-same-agent" value="true" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

